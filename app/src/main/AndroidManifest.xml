<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.jaiminshah.easystartup">
    <!-- To auto-complete the email text field in the login form with the user's emails -->
    <uses-permission android:name="android.permission.GET_ACCOUNTS" />
    <uses-permission android:name="android.permission.READ_PROFILE" />
    <uses-permission android:name="android.permission.READ_CONTACTS" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity
            android:name=".invesor_bottom_navigation_activity"
            android:label="@string/title_activity_invesor_bottom_navigation_activity"></activity>
        <activity
            android:name=".jobseekeer_bottom_navigation"
            android:label="@string/title_activity_jobseekeer_bottom_navigation" />
        <activity
            android:name=".enterprise_bottom_navigation"
            android:label="@string/title_activity_enterprise_bottom_navigation" />
        <activity android:name=".activity_company_listpage" />
        <activity android:name=".activity_company_profile" />
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".SignUpPageforEnterprise" />
        <activity android:name=".signinactivityforenterprise" />
        <activity android:name=".SignupPageforJobseeker" />
        <activity android:name=".SigninActivityforJobSeeker" />
        <activity android:name=".Jobseeker_ListActivity" />
        <activity android:name=".PromptPage" />
        <activity android:name=".signInpageforInvestor" />
        <activity android:name=".signUpPageforInvestor" />
        <activity android:name=".Jobseeker_ProfileActivity" />
    </application>

</manifest>